<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hrc.administrator.myapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListViewActivity"></activity>
        <activity android:name=".AddDataActivity"></activity>
        <activity android:name=".DialogActivity"
            android:theme="@style/Theme.AppCompat.Dialog"></activity>
        <activity android:name=".SelectActivity"></activity>
        <activity android:name=".UpdateActivity"></activity>
        <activity android:name=".AlertDialogActivity"/>
        <activity android:name=".FindPass"/>
    </application>

</manifest>
